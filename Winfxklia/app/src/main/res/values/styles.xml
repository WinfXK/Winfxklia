<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="winfxkliba_alert_dialog1" parent="winfxkliba_dialog">
        <item name="android:windowEnterAnimation">@anim/winfxkliba_actionsheet_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/winfxkliba_actionsheet_dialog_out</item>
    </style>

    <style name="winfxkliba_alert_dialog2" parent="winfxkliba_dialog">
        <item name="android:windowEnterAnimation">@anim/winfxkliba_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/winfxkliba_dialog_out</item>
    </style>

    <style name="winfxkliba_dialog" parent="android:Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowAnimationStyle">@android:anim/accelerate_decelerate_interpolator</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>

    <style name="winfxkliba_listDialog" parent="winfxkliba_dialog">
        <item name="android:gravity">bottom</item>
        <item name="android:windowEnterAnimation">@anim/winfxkliba_listdialog_in</item>
        <item name="android:windowExitAnimation">@anim/winfxkliba_listdialog_out</item>
    </style>

    <style name="winfxkliba_progress_dialog" parent="winfxkliba_dialog">
        <item name="android:windowEnterAnimation">@anim/winfxkliba_progress_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/winfxkliba_progress_dialog_out</item>
    </style>
</resources>